<?xml version="1.0" encoding="utf-8"?>
<!-- Place comic_sans_regular.ttf, comic_sans_bold.ttf, and consolas.ttf in res/font/ -->
<!-- Font file names must be lowercase with underscores in Android -->
<font-family xmlns:android="http://schemas.android.com/apk/res/android">
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/comic_sans_regular" />
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/comic_sans_bold" />
</font-family>
